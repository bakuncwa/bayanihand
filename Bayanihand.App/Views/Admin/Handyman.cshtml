@model List<InquiryINV>

<h3>Handyman Follow Ups</h3>
<hr />

<div class="alert alert-info">
    @Model.Count result(s) found
</div>
<br />

<table class="table table-striped table-hover table-bordered table-sm">
    <thead>
        <tr>
            <th>Inquiry I.D.</th>
            <th>Date Inquired</th>
            <th>Title</th>
            <th>Handyman I.D.</th>
            <th>Description</th>
            <th>Action</th>
        </tr>
    </thead>
    <tbody>
        @foreach (InquiryINV item in Model)
        {
            <tr>
                <td>@item.InquiryID</td>
                <td>@item.DateInquired</td>
                <td>@item.Title</td>
                <td>@item.HandymanID</td>
                <td>@item.Description</td>
                <td>
                <a asp-controller="Admin" asp-action="Delete" asp-route-Id="@item.HandymanID"
                    class="btn btn-outline-warning btn-sm">
                        Delete
                </a>
            </td>
        </tr>
        }
    </tbody>
 </table>
